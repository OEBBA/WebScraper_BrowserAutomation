{
    "name": "Scraper",
    "version": "0.01",
    "description": "Web Scraper",
    "permissions": ["<all_urls"],
    "browser_action": {
        "default_title": "Scraper",
        "default_popup": "popout.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ],
            "run_at": "document_end"
        }
    ],
    "manifest_version": 2
}